# {{ ansible_managed }}
[Unit]
Description=Hypothesis Celery Beat
After=network.target

[Service]
Type=forking
User=www-data
EnvironmentFile=-/etc/default/h
WorkingDirectory={{ openstax_hypothesis_role.application_dir }}
ExecStart={{ openstax_hypothesis_role.application_dir }}/bin/hypothesis celery beat 
StandardOutput=syslog
StandardError=syslog
Type=simple
RestartSec=1

[Install]
WantedBy=multi-user.target
